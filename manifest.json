{
  "name": "Zentune",
  "author": {
    "name": "Tommy Porowski",
    "email": "tporowski17@gmail.com",
    "url": "https://tporowski17.github.io/tommyautomation"
  },
  "defaultLocale": "en",
  "private": false,
  "location": {
    "support": {
      "ticket_sidebar": {
        "url": "assets/iframe.html",
        "flexible": true
      }
    }
  },
  "version": "1.0.0",
  "frameworkVersion": "2.0",
  "parameters": [
    {
      "name": "zendesk_subdomain",
      "type": "text",
      "label": "Zendesk Subdomain",
      "helpText": "Found in url: https://{subdomain}.zendesk.com",
      "required": true
    },
    {
      "name": "azure_client_id",
      "type": "text",
      "required": true,
      "helpText": "This is the Client ID Generated by Azure App Registration.",
      "label": "Azure Client ID"
    },
    {
      "name": "azure_tenant_id",
      "type": "text",
      "required": true,
      "helpText": "This is the Tenant ID Generated by Azure App Registration.",
      "label": "Azure Tenant ID"
    }
  ]
}